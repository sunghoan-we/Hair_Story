<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/data_search_member_branches_bID"
        android:layout_width="0dp"
        android:layout_height="60sp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:gravity="center_vertical"
        android:text="1"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/data_search_member_branches_bName"
        android:layout_width="0dp"
        android:layout_height="20sp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:gravity="center_vertical"
        android:text="Branch Name"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/data_search_member_branches_bID"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/data_search_member_branches_contact"
        android:layout_width="0dp"
        android:layout_height="20sp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:gravity="center_vertical"
        android:text="This is contact"
        app:layout_constraintStart_toEndOf="@+id/data_search_member_branches_bName"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/data_search_member_branches_address"
        android:layout_width="0dp"
        android:layout_height="20sp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:gravity="center_vertical"
        android:text="This is address"
        app:layout_constraintStart_toEndOf="@+id/data_search_member_branches_bID"
        app:layout_constraintTop_toBottomOf="@+id/data_search_member_branches_bName" />

    <TextView
        android:id="@+id/data_search_member_branches_time"
        android:layout_width="0dp"
        android:layout_height="20sp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="10dp"
        android:gravity="center_vertical"
        android:text="This is time"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@+id/data_search_member_branches_bID"
        app:layout_constraintTop_toBottomOf="@+id/data_search_member_branches_address" />

    <TextView
        android:id="@+id/btn_search_member_branches_reservation"
        android:layout_width="120sp"
        android:layout_height="60sp"
        android:layout_marginStart="250dp"
        android:layout_marginLeft="250dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/teal_700"
        android:gravity="center"
        android:text="SELECT\nDESIGNER"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/data_search_member_branches_bID"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>