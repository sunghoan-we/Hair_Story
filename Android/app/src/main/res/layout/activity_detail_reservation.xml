    <EditText
        android:id="@+id/input_reservation_member_make_date"
        android:layout_width="0dp"
        android:layout_height="30sp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:background="#EDEDED"
        android:ems="10"
        android:gravity="left|center_vertical"
        android:inputType="textPersonName"
        android:paddingLeft="10sp"
        android:paddingRight="10sp"
        android:text="09:00 1st. Dec. 2021"
        android:textSize="14sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/label_reservation_member_make_date"
        app:layout_constraintTop_toBottomOf="@+id/input_reservation_member_make_date2" />

    <TextView
        android:id="@+id/input_reservation_member_make_date5"
        android:layout_width="0dp"
        android:layout_height="30sp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:background="#EDEDED"
        android:ems="10"
        android:gravity="left|center_vertical"
        android:inputType="textPersonName"
        android:paddingLeft="10sp"
        android:paddingRight="10sp"
        android:text="Shop Name"
        android:textSize="14sp"
        app:layout_constraintEnd_toStartOf="@+id/input_reservation_member_make_date6"
        app:layout_constraintStart_toEndOf="@+id/label_reservation_member_make_date5"
        app:layout_constraintTop_toBottomOf="@+id/input_reservation_member_make_dName" />

    <TextView
        android:id="@+id/label_reservation_member_make_date5"
        android:layout_width="100sp"
        android:layout_height="30sp"
        android:layout_below="@+id/label_reg_shopAdmin_ID"
        android:layout_alignParentStart="true"
        android:layout_alignParentLeft="true"
        android:layout_marginStart="10dp"
