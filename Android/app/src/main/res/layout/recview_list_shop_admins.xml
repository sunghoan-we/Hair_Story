<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/txt_list_shopAdmin_ID"
        android:layout_width="0dp"
        android:layout_height="20sp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:text="Markgraykim"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_list_shopAdmin_bar2"
        android:layout_width="0dp"
        android:layout_height="20sp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:text="|"
        app:layout_constraintStart_toEndOf="@+id/txt_list_shopAdmin_ID"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_list_shopAdmin_Name"
        android:layout_width="wrap_content"
        android:layout_height="20sp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:text="(Junhyung Kim)"
        app:layout_constraintStart_toEndOf="@+id/txt_list_shopAdmin_bar2"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_list_shopAdmin_email"
        android:layout_width="0dp"
        android:layout_height="20sp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="10dp"
        android:text="email address"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_list_shopAdmin_ID" />

    <TextView
        android:id="@+id/txt_list_shopAdmin_bar3"
        android:layout_width="0dp"
        android:layout_height="20sp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:text="|"
        app:layout_constraintStart_toEndOf="@+id/txt_list_shopAdmin_email"
        app:layout_constraintTop_toBottomOf="@+id/txt_list_shopAdmin_Name" />

    <TextView
        android:id="@+id/label_list_shopAdmin_regDate"
        android:layout_width="0dp"
        android:layout_height="20sp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:text="Reg. date: "
        app:layout_constraintStart_toEndOf="@+id/txt_list_shopAdmin_bar3"
        app:layout_constraintTop_toBottomOf="@+id/txt_list_shopAdmin_Name" />

    <TextView
        android:id="@+id/txt_list_shopAdmin_regDate"
        android:layout_width="wrap_content"
        android:layout_height="20sp"
        android:layout_marginEnd="10dp"
        android:layout_marginRight="10dp"
        android:text="20202020"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/label_list_shopAdmin_regDate"
        app:layout_constraintTop_toBottomOf="@+id/txt_list_shopAdmin_Name" />

</androidx.constraintlayout.widget.ConstraintLayout>